                    "content": {
                      "type": "string"
                    },
                    "published": {
                      "type": "boolean"
                    },
                    "updatedAt": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "id",
                    "title",
                    "content",
                    "published",
                    "updatedAt"
