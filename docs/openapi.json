    "/actions/updatePost": {
      "post": {
        "summary": "Update a post",
        "description": "Updates an existing post. Requires authentication and authorization (user must own the post). The action validates both input and output schemas to ensure data integrity.",
        "tags": [
          "posts"
        ],
        "operationId": "updatePost",
        "security": [
          {
            "bearerAuth": []
