    "validate-links": "tsx scripts/validate-links.ts",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "feed": "^5.1.0",
    "fumadocs-core": "16.4.1",
    "fumadocs-mdx": "14.2.3",
    "fumadocs-openapi": "^10.2.2",
    "fumadocs-typescript": "^5.0.0",
    "fumadocs-ui": "16.4.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "server-only": "^0.0.1",
    "shiki": "^3.20.0",
